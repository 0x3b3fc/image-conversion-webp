{
    "name": "phpsamurai/image-conversion",
    "description": "A simple Laravel ^10 package for converting media images to WebP format",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Sief Hesham",
            "email": "dev.sief.hesham@gmail.com"
        }
    ],
    "keywords": [
        "php",
        "laravel",
        "webp",
        "filament",
        "splade",
        "phpsamurai",
        "images",
        "png",
        "jpg",
        "jpeg"
    ],
    "require": {
        "php": "^8.0",
        "illuminate/support": "^10.0",
        "spatie/image": "*",
        "spatie/laravel-medialibrary": "*"
    },
    "autoload": {
        "psr-4": {
            "phpsamurai\\ImageConversion\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "phpsamurai\\ImageConversion\\ImageConversionServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
